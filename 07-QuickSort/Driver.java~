import java.util.Random;
public class Driver{

    public static void main(String[]args){
	Random rand = new Random();
	int[] a = new int[10];
	for(int x = 0; x < 10; x ++)
	    a[x] = rand.nextInt(21);

	QuickSort b = new QuickSort();
	b.quickSort(a);
	System.out.println(Arrays.toString(a));

    }


}
